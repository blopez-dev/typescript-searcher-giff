{
  "name": "typescript-searcher-giff",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint:check": "npm run lint -- --no-error-on-unmatched-pattern",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint \"./{src,scripts,test}/**/*.{ts,js,tsx,jsx}\" --format @inditex/eslint-formatter-inditex",
    "preview": "vite preview",
    "format": "npm run lint -- --fix --fix-type",
    "prepare": "cd .. && husky install code/.husky",
    "test": "jest"
  },
  "lint-staged": {
    "(config|src|test)/**/*.(js|jsx|ts|tsx)": [
      "npm run lint:check"
    ]
  },
  "dependencies": {
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.3",
    "ts-loader": "^9.5.1",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "devDependencies": {
    "@inditex/eslint-config": "^1.7.0",
    "@types/jest": "^29.5.11",
    "@types/prop-types": "^15.7.11",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "overrides": {
    "eslint-plugin-prettier": "5.0.0"
  },
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
