{
  "name": "giffexpertapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint \"./{src,scripts,test}/**/*.{ts,js,tsx,jsx}\" --format --no-error-on-unmatched-pattern",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "cd .. && husky install code/.husky",
    "lint:check": "npm run lint",
    "lint:fix": "eslint \"./{src,scripts,test}/**/*.{ts,js,tsx,jsx}\" --fix",
    "format": "eslint \"./{src,scripts,test}/**/*.{ts,js,tsx,jsx}\" --fix --fix-type layout"
  },
  "lint-staged": {
    "(config|src|test)/**/*.(js|jsx|ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "overrides": {
    "eslint-plugin-prettier": "5.1.1"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "eslint-plugin-prettier": "^5.1.1",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "typescript": "*",
    "vite": "^5.0.8"
  }
}
